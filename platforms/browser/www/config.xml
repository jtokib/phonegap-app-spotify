<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.phonegapspotify" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PhoneGap Spotify - Short Task</name>
    <description>
        A basic Framework7 Media App for PhoneGap using the Spotify API.
    </description>
    <author email="jburke9@mail.ccsf.edu">
        Toki Burke
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="phonegap-version" value="cli-8.2.2" />
</widget>
